<?xml version="1.0" encoding="utf-8"?>
<Containers>
    <Beta>False</Beta>
    <Category>Network:Other</Category>
    <Name>OnlyOffice-DocumentServer</Name>
    <Description>OnlyOffice Document Server</Description>

    <GitHub>https://github.com/ONLYOFFICE/Docker-DocumentServer</GitHub>
    <Repository>onlyoffice/documentserver</Repository>
    

    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
        		<HostPort></HostPort>
        		<ContainerPort></ContainerPort>
        		<Protocol></Protocol>
        	</Port>
		</Publish>
    </Networking>
    <Data>
    	<Volume>
    		<HostDir>/mnt/user/appdata/onlyoffice/DocumentServer/data</HostDir>
    		<ContainerDir>/var/www/onlyoffice/Data</ContainerDir>
    		<Mode>rw</Mode>
    	</Volume>
    	<Volume>
    		<HostDir>/mnt/user/appdata/onlyoffice/DocumentServer/logs</HostDir>
    		<ContainerDir>/var/log/onlyoffice</ContainerDir>
    		<Mode>rw</Mode>
    	</Volume>
    </Data>
    <Icon>http://a4.mzstatic.com/us/r30/Purple5/v4/af/6e/2c/af6e2cd3-974c-f62c-f754-ea1264ec051b/icon175x175.jpeg</Icon>
    <ExtraParams>-i -t</ExtraParams>
</Containers>
